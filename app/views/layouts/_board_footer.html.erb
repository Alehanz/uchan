<footer class="board-footer">
  <div class="boardNav">
    <span class="boardList">
      <span class="braces">[</span>
      <% @boards.each do |board| %>
        <span class="braces">/<%= link_to board.name, board_path(board) %></span>
      <% end %>
      <span class="braces">]</span>
      <span class="braces">[<%= link_to "Home", root_path %>]</span>
    </span>
  </div>
</footer>
